{
  "manifestVersion": "0.1.0",
  "message": "More at https://github.com/wmitsuda/oz-contract-starter",
  "files": [
    ".gitignore",
    ".nvmrc",
    ".dependabot/config.yml",
    "package.json",
    "tsconfig.json",
    "buidler.config.ts",
    ".openzeppelin/project.json",
    "contracts",
